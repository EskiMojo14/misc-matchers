{
  "name": "mix-n-matchers",
  "version": "1.1.2",
  "description": "Miscellaneous custom Jest matchers",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./all": {
      "import": "./dist/all.js",
      "require": "./dist/all.cjs"
    },
    "./jest-globals": {
      "import": "./dist/jest-globals.js",
      "require": "./dist/jest-globals.cjs"
    },
    "./vitest": {
      "import": "./dist/vitest.js",
      "require": "./dist/vitest.cjs"
    }
  },
  "files": [
    "dist",
    "all",
    "jest-globals",
    "vitest"
  ],
  "scripts": {
    "prepare": "husky install",
    "prebuild": "yarn run type",
    "build": "tsup",
    "test": "yarn node --experimental-vm-modules $(yarn bin jest)",
    "vitest": "vitest",
    "lint": "eslint",
    "pre-commit": "lint-staged",
    "attw": "attw",
    "publint": "publint",
    "type": "tsc",
    "prepack": "yarn run publint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EskiMojo14/mix-n-matchers.git"
  },
  "keywords": [
    "jest",
    "matchers",
    "extend",
    "extended",
    "test",
    "testing",
    "assertions"
  ],
  "author": "eskimojo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EskiMojo14/mix-n-matchers/issues"
  },
  "homepage": "https://github.com/EskiMojo14/mix-n-matchers#readme",
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.13.2",
    "@types/jest": "^29.5.5",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.49.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.4.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-matcher-utils": "^29.7.0",
    "lint-staged": "^15.0.0",
    "prettier": "^3.0.3",
    "publint": "^0.2.2",
    "ts-jest": "^29.1.1",
    "tsup": "^8.0.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "peerDependencies": {
    "jest": ">=28.0.0"
  },
  "peerDependenciesMeta": {
    "jest": {
      "optional": true
    }
  },
  "engines": {
    "node": "^14.15.0 || ^16.10.0 || >=18.0.0"
  },
  "jest": {
    "preset": "ts-jest/presets/js-with-ts-esm",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./jest-setup.ts"
    ],
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "testPathIgnorePatterns": [
      "src/(.*/)*.*\\.vi\\.test\\.ts"
    ]
  },
  "lint-staged": {
    "*.{ts,md}": "prettier --write"
  },
  "prettier": {},
  "tsup": {
    "entry": [
      "src/index.ts",
      "src/all.ts",
      "src/jest-globals.ts",
      "src/vitest.ts"
    ],
    "sourcemap": true,
    "format": [
      "esm",
      "cjs"
    ],
    "dts": true,
    "minify": true,
    "external": [
      "@jest/globals",
      "vitest"
    ]
  }
}
